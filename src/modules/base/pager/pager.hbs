<ul class="pager">
    <li>
        <a class="pager-anchor" href="#">
            <span >&laquo;</span>
        </a>
    </li>

    {{#for_n 10}}
        <li>
            <a class="pager-anchor {{#eq this 4}}--active{{/eq}}" href="#">
                {{sum this 1}}
            </a>
        </li>
    {{/for_n}}

    <li>
        <a class="pager-anchor" href="#">
            <span>&raquo;</span>
        </a>
    </li>
</ul>
