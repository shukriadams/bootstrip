{{#extend "_layout"}}
    {{#content "body"}}
        <div class="dashboardDemo-page">
            <div class="dashboardDemo-pageNavLeft">
                {{>sideMenu items=sideMenu}}
            </div>

            <div class="dashboardDemo-pageNotNavLeft">
                <div class="dashboardDemo-pageTopMenu">
                    
                </div>
                <div class="dashboardDemo-pageGeneralContent">
                    
                    <x-cards>
                        <x-columns3>
                            {{>card cards.multiGreen}}
                            {{>card cards.multiOrange}}
                            {{>card cards.multiRed}}
                        </x-columns3>

                        <x-columns2>
                            {{>card cards.all}}
                            {{>card cards.all}}
                        </x-columns2>

                        <x-columns2>
                            <x-card>
                                <x-card-content>
                                    <x-strip>
                                        <x-strip-item><i class="icon icon-"></i></x-strip-item>
                                        <x-strip-item>Some stuff</x-strip-item>
                                        <x-strip-item>Some stuff</x-strip-item>
                                    </x-list-row>
                                </x-card-content>
                            </x-card>
                            <x-card>
                                <x-card-content>
                                    <x-strip>
                                        <x-strip-item>Some stuff</x-strip-item>
                                        <x-strip-item>Some stuff</x-strip-item>
                                        <x-strip-item>Some stuff</x-strip-item>
                                    </x-list-row>
                                </x-card-content>
                            </x-card>
                        </x-columns2>

                        <x-columns4>
                            {{>card cards.all}}
                            {{>card cards.all}}
                            {{>card cards.all}}
                            {{>card cards.all}}
                        </x-columns4>
                    </x-cards>

                </div>
            </div>

        </div>
    {{/content}}
{{/extend}}
