{{#>kitchenSinkItem title="Header"}}
    {{>header title="MyHeader" items=header.items}}
{{/kitchenSinkItem}}

{{#>kitchenSinkItem title="Footer"}}
    {{>footer}}
{{/kitchenSinkItem}}

{{#>kitchenSinkItem title="Columns" description="Column can be used to create a responsive grid" }}
    {{>columns}}
{{/kitchenSinkItem}}

{{#>kitchenSinkItem title="Typography" }}
    {{>typography}}
{{/kitchenSinkItem}}

{{#>kitchenSinkItem title="Compact select"}}
<x-columns class="--two">
    <div></div>
    <div>{{>compactSelect}}</div>    
</x-columns>

{{/kitchenSinkItem}}

{{#>kitchenSinkItem title="Circle graphs"}}
    {{>circleGraphs}}
{{/kitchenSinkItem}}

{{#>kitchenSinkItem title="Image"}}
    {{>image}}
{{/kitchenSinkItem}}

{{#>kitchenSinkItem title="Cover"}}
    {{>cover}}
{{/kitchenSinkItem}}

{{#>kitchenSinkItem title="Figure"}}
    {{>figure caption="my caption" src="http://placehold.it/1280x720"}}
{{/kitchenSinkItem}}

{{#>kitchenSinkItem title="List" description="Lists are ideal for simple collections of data"}}
    {{>list rows=listData.items.rows }}
{{/kitchenSinkItem}}

{{#>kitchenSinkItem title="List with stripes" }}
    {{>list modifier="--striped" rows=listData.items.rows }}
{{/kitchenSinkItem}}

{{#>kitchenSinkItem title="List with no width constrants" }}
    {{>list modifier="--freewidth" rows=listData.items.rows }}
{{/kitchenSinkItem}}


{{#>kitchenSinkItem title="List" description="Lists can be compacted for denser data"}}
    {{>list rows=listData.items.rows modifier="dense" }}
{{/kitchenSinkItem}}

{{#>kitchenSinkItem title="Table" description="Use tables for more fine-tuned control of data layout"}}
    <x-content>
        {{>table table.items}}
    </x-content>
    <x-content>
        {{>table tableModifiers.items}}
    </x-content>
{{/kitchenSinkItem}}

{{#>kitchenSinkItem title="Alerts"}}
    {{>alerts}}
{{/kitchenSinkItem}}

{{#>kitchenSinkItem title="Steenking Badgers"}}
    {{>badges}}
{{/kitchenSinkItem}}

{{#>kitchenSinkItem title="Buttons"}}
    {{>buttons}}
{{/kitchenSinkItem}}

{{#>kitchenSinkItem title="Form"}}
    {{>form}}

    <h2>Form Grid</h2>
    {{>formgrid}}
{{/kitchenSinkItem}}

{{#>kitchenSinkItem title="Cards"}}
    {{>cards}}
{{/kitchenSinkItem}}

{{#>kitchenSinkItem title="Pager"}}
    {{>pager}}
{{/kitchenSinkItem}}

{{#>kitchenSinkItem title="Side Menu"}}
    {{>sideMenu items=sideMenu}}
{{/kitchenSinkItem}}

{{#>kitchenSinkItem title="Icon strip"}}
    {{>iconStrip items=iconStrip.all}}
{{/kitchenSinkItem}}


{{#>kitchenSinkItem title="Code"}}
    {{>code}}
{{/kitchenSinkItem}}